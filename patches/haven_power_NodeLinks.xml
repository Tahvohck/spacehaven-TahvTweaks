<Patch>
	<Operation Class="AttributeSet">
		<!--Wall (882) and floor (2292) power nodes.-->
		<xpath>/data/Element/me[@mid="2292" or @mid="882"]/data/l/element/features/powerGrid</xpath>
		<attribute>maxLinks</attribute>
		<value>4</value>
	</Operation>
	<Operation Class="AttributeSet">
		<!--Standing (33) power nodes.-->
		<xpath>/data/Element/me[@mid="33"]/data/l/element/features/powerGrid</xpath>
		<attribute>maxLinks</attribute>
		<value>6</value>
	</Operation>
</Patch>